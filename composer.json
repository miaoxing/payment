{
  "name": "miaoxing/payment",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Payment Plugin",
  "keywords": [
    "miaoxing",
    "payment"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "^0.7.9",
    "miaoxing/product": "^0.4.24"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Payment\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Payment\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "require-ci": {
      "miaoxing/dev": "miaoxing/dev as 8.x-dev",
      "miaoxing/phpstan-miaoxing": "miaoxing/phpstan-miaoxing as 2.x-dev"
    }
  },
  "require-dev": {
    "miaoxing/dev": "^8.2.4"
  }
}
