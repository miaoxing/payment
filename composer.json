{
  "name": "miaoxing/payment",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Payment Plugin",
  "keywords": [
    "miaoxing", "payment"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "^0.2.2",
    "miaoxing/product": "^0.1.0"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Payment\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Payment\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-types": [
      "miaoxing-plugin"
    ],
    "installer-paths": {
      "plugins/{$name}/": [
        "type:miaoxing-plugin"
      ]
    }
  }
}
